<div class="logo"><a href="/blog">changliu</a></div>

<div class="lines-button-container">
  <button type="button" role="button" aria-label="Toggle Navigation" class="lines-button x2">
    <span class="lines"></span>
  </button>
</div>

<div class="main-menu">
  <div class="inner">
    <div class="menu-content">
      <h2>Menu</h2>
      {{!-- <a href="#" class="nav-close">
        <span class="hidden">Close</span>
      </a> --}}
      <h3>Navigation</h3>
      <ul>
        {{#foreach navigation}}
        <li class="nav-{{slug}}{{#if current}} nav-current{{/if}}" role="presentation"><a href="{{url absolute="true"}}">{{label}}</a></li>
        {{/foreach}}
        {{#if @labs.subscribers}}
        <li><a class="subscribe-button" href="{{@blog.url}}/subscribe/">Subscribe</a></li>
        {{else}}
        <li><a class="subscribe-button icon-feed" href="{{@blog.url}}/rss/">Subscribe</a></li>
        {{/if}}
      </ul>
      
      <h3>Tags</h3>
      <ul>
        <li>#photo</li>
        <li>#code</li>
        <li>#travel</li>
        <li>#company-tours</li>
      </ul>
      <h3>Elsewhere</h3>
